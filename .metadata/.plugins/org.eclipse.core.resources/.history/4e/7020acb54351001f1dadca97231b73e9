package com.aurionpro.behaviour.command.model;

public class TurnOffCommand implements ICommand{
	
	AirConditoner ac;
	
	public TurnOffCommand(AirConditoner ac) {
		super();
		this.ac = ac;
	}

	
	public AirConditoner getAc() {
		return ac;
	}


	public void setAc(AirConditoner ac) {
		this.ac = ac;
	}

	@Override
	public void execute() {
		// TODO Auto-generated method stub
		ac.turnOff();
		
		
	}
	
}
