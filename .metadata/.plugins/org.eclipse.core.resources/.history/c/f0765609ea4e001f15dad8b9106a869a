public class FindGuitarTester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Inventory inventory = new Inventory();
		intializeInventory(inventory);
		GuitarSpec whatErinLikes = new GuitarSpec("fender", "Stracastor", "electric", "Alder", "Alder");
		
		List<GuitarSpec> matchingGuitars = inventory.search(whatErinLikes);
		if(!matchingGuitars.isEmpty()) {
			System.out.println(matchingGuitars);
			return;
		}
		System.out.println("Sorry, Guitar is not found");
	}
	private static void intializeInventory(Inventory inventory) {
		
	}

}
