package com.aurionpro.builder.test;

import com.aurionpro.builder.model.User;
import com.aurionpro.builder.model.User.UserBuilder;

public class UserTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		User user = new User.UserBuilder()
							.setEmailId("Shirish@gmail.com")
							.setUserId("UserId123")
							.setUserName("Shirish")
							.build();
		
		
//		for Static method do not need to use new keyword 
		
		UserBuilder user2 = User.UserBuilder.builder().setEmailId("Shirish@gmail.com").setUserName("Shirish");
		System.out.println(user2);
		
		
		System.out.println(user);

	}

}
